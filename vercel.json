{
  "rewrites": [{ "source": "/sitemap.xml", "destination": "/api/generate-sitemap" }],
  "functions": {
    "api/generate-sitemap.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "crons": [
    {
      "path": "/src/utils/generate-sitemap.ts",
      "schedule": "52 08 * * *"
    }
  ]
}
