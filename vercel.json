{
  "rewrites": [{ "source": "/sitemap.xml", "destination": "/src/utils/generate-sitemap.ts" }],
  "functions": {
    "src/utils/generate-sitemap.ts": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "crons": [
    {
      "path": "/src/utils/generate-sitemap.ts",
      "schedule": "52 08 * * *"
    }
  ]
}
